prefix=@PREFIX@
exec_prefix=${prefix}
libdir=${prefix}/@LIBPREFIX@
includedir=${prefix}/include/taiwins

Name: twclient-nkegl
Description: nuklear GUI library with EGL backend for twclient
Version: @VERSION@

Requires: twclient, twclient-icons, wayland-client, xkbcommon, fontconfig, freetype2, wayland-egl, wayland-cursor, egl, gl, glesv2, librsvg-2.0
Libs: -L${libdir} twclient-nkegl
Cflags: -I${includedir}

